@model IEnumerable<Tourismpk.Models.Tourist>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@using (Html.BeginForm("LogIn", "Admins"))
{
    @Html.AntiForgeryToken()
    <div class="col-sm-9 col-sm-offset-3 col-lg-10 col-sm-offset-2 main">
        <div class="container mt-5 ">
            <h3 class="text-center"> Log In </h3>
            <div class="row">

                <div class="col-md-5">

                    <div class="form-group">
                        <label for="">Email</label>
                        <input type="email" name="Admin_Email" required class="form-control">
                    </div>
                    <div class="form-group">
                        <label for="">Password</label>
                        <input type="password" name="Admin_Password" required class="form-control">

                    </div>


                    <div class="form-group ">

                        <input type="submit" value="Log In" required class="btn btn-danger form-control">
                    </div>
                </div>
            </div>
        </div>
    </div>}
